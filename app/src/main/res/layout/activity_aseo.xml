<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.tdc.trabajandodesdecasa.Aseo"
    style="@style/Madres"
    android:id="@+id/contenedor">

    <Button
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/lavar"
        android:layout_marginTop="40dp"
        android:background="@mipmap/lavar"
        android:layout_marginRight="0dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/plachar"
        android:layout_marginEnd="37dp" />

    <Button
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:id="@+id/plachar"
        android:background="@mipmap/planchar"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="58dp"
        android:layout_alignTop="@+id/lavar"
        android:layout_alignParentEnd="true" />

    <RelativeLayout
        style="@style/Encabezado"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:id="@+id/relativeLayout6"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:id="@+id/linearLayout5"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="185dp" />

        <Button
            android:layout_width="40dp"
            android:layout_height="wrap_content"
            android:id="@+id/button11"
            android:background="@mipmap/ic_launcher"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Lavar"
        android:id="@+id/Lavar"
        android:gravity="center_horizontal"
        android:textStyle="bold"
        android:layout_below="@+id/lavar"
        android:layout_alignStart="@+id/lavar"
        android:layout_alignEnd="@+id/lavar" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Planchar"
        android:id="@+id/Planchar"
        android:gravity="center_horizontal"
        android:textStyle="bold"
        android:layout_alignStart="@+id/plachar"
        android:layout_alignTop="@+id/Lavar"
        android:layout_alignEnd="@+id/plachar" />

</RelativeLayout>
